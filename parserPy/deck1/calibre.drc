// ----------------------------------------------------------------------------
//        minimum pad opening window  for  DEVICE  CLASS  N1
// ----------------------------------------------------------------------------

R1_N1 { @ Rule 1 (Device Class N1) : minimum pad opening window >= 44.000 x 108.000
      PAD_NOTCH_FILLED = EXTENT_PADN1 OR (EXT PADN1 <= 3.000 NOTCH REGION)
    BPADR    = NOT RECTANGLE PAD_NOTCH_FILLED >= 44.000 BY >=108.000
      BWPADP   = INT PAD_NOTCH_FILLED < 44.000 REGION OPPOSITE ABUT>0<90
      GWPADP   = PAD_NOTCH_FILLED OUTSIDE BWPADP
    BLPADP:1 = INT [GWPADP] < 108.000 OPPOSITE ABUT>0<90
      BLPADP   = GWPADP WITH EDGE BLPADP:1 >2
      BPADR OR ( BWPADP OR BLPADP )
    }

R1imp_N1 {@  Rule R1imp (Device Class N1) : 45 degrees notches are forbidden on pad opening
    X = ANGLE (EXTENT_PADN1 NOT PADN1) ==45
    (EXTENT_PADN1 NOT PADN1) WITH EDGE X >1
    }

R3_N1 { @ Rule 3 (Device Class N1) : maximum size of chamfer in the corner of pad is 10.000um in both direction
    X= EXTENT_PADN1 NOT PADN1
    Y = COINCIDENT EDGE X EXTENT_PADN1
    LENGTH Y > 10.000
    }
RA_N1 { @ Rule A (Device Class N1) : upper metal min enclosure of nitride 2.000
      ENC PADN1 THICKMETAL_N1 < 1.995 OUTSIDE ALSO
    } 

RC_N1 { @ Rule C (Device Class N1) : min Al to Al distance : 2.000
      X = SIZE EXTENT_PADN1 BY 4.000
    Y = (THICKMETAL_N1 INTERACT PADN1) AND X
    Z = Y OR (HOLES Y)
    EXT Z < 2.000 SPACE
    } 

