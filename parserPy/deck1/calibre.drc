GATE_ESD= GATE OR (GATE INTERACT MKR_esd2) 
GATE_NOT_ESD = GATE_ESD NOT MKR_esd2

RPO.R.3 { @ Rule RPO.R.3: Partially non-siliced gate must be covered by MKR_ESD2
        X = GATE CUT (RPO NOT FILTER_RPO.R.3)
    X NOT MKR_esd2
    }
    
RPO.OL.1 { @ RPO.OL.1  RPO overlap of Gate if covered by ESD marker == 0.060 um
      X = GATE_ESD AND MKR_esd2
    INT X (RPO NOT FILTER_RPO.OL.1) < 0.060
    }


NWDrift = NWEL INTERACT ODDrift
NWODDrift = OD_drg INTERACT NWDrift
ODDriftExt = EXTENTS ((NWODDrift OUTSIDE DNW) AND NWDrift)
ODDriftSized = SIZE ODDriftExt BY 1
ODDriftMerged = SIZE ODDriftExt BY 1 OVERUNDER

DR.EX.2 { @ Rule DR.EX.2: NW Extension on OD in ST drifts: min=max=1
    ENC ODDriftMerged NWDrift < 1 SINGULAR ABUT >0<90 REGION //min checked
    NWThick = NWDrift NOT ODDriftSized 
//if correct, NWThick is empty or is a rectangle between Drain and Gate.
    NOT RECTANGLE NWThick
    NWThick NOT INTERACT ODDriftSized ==2
    }

DR_PWB_BLK = PW_bk INTERACT NWDrift

DR.W.1 { @ Rule DR.W.1 : PWB.blk width min = max : 1.5
    INT DR_PWB_BLK < 1.5 ABUT>0<90 SINGULAR REGION
    SIZE DR_PWB_BLK BY 0.75 UNDEROVER
    }

GATE_W_MTP_C1       = (POLY AND MKR_mtp) COIN INSIDE EDGE GATE      // Gate width
HV_GATE_W_25_MTP_C1    = GATE_W_MTP_C1 INSIDE EDGE OD2_25_dg

PO.W.4a { @ Rule PO.W.4a  Transistor length for 2.5V NMOS and PMOS 0.13um inside MTP cell
      INT HV_GATE_W_25_MTP_C1 < 0.13  ABUT <90 REGION
    }

PO.W.6a { @ Rule PO.W.6a: Width of 45degrees PO lines on field oxide:  0.13um  inside MTP cell
      INT ((POLY NOT POLYG_dm) AND MKR_mtp)  < 0.13 ABUT <90 ANGLED REGION
    }

RPO.A.2a { @ RPO.A.2a  RPO Area 0.33 um2 inside MTP cell
      HOLES (RPO  AND MKR_mtp) < 0.33
    }



IDC_L.1  { @ Rule IDC_L.1 Max length : 2.0
    LENGTH VTH_P_ebeam > 2.0
    }

IDC_R.1  { @ Rule IDC_R.1  Max height of the Boundary box of all VTH_P_ebeam;ebeam layers 630
    X = EXTENT VTH_P_ebeam
    Y = SHRINK X TOP BY 315 BOTTOM BY 315
    X INTERACT Y    
    }    
 
//------------------------------ metal1 user inductor device
INDUCTOR_M1 = ((M1 AND M1_markerLA) OR ( M1 AND  M1_markerLS)) OR ( M1 AND M1_markerL)         
 
//------------------------------ metal2 user inductor device
INDUCTOR_M2 = ((M2 AND M2_markerLA) OR ( M2 AND  M2_markerLS)) OR ( M2 AND M2_markerL)         
 
//------------------------------ metal3 user inductor device
INDUCTOR_M3 = ((M3 AND M3_markerLA) OR ( M3 AND  M3_markerLS)) OR ( M3 AND M3_markerL)         
 
//------------------------------ metal4 user inductor device
INDUCTOR_M4 = ((M4 AND M4_markerLA) OR ( M4 AND  M4_markerLS)) OR ( M4 AND M4_markerL)         
 
//------------------------------ metal5 user inductor device
INDUCTOR_M5 = ((M5 AND M5_markerLA) OR ( M5 AND  M5_markerLS)) OR ( M5 AND M5_markerL)         
 
//------------------------------ metal6 user inductor device
INDUCTOR_M6 = ((M6 AND M6_markerLA) OR ( M6 AND  M6_markerLS)) OR ( M6 AND M6_markerL)         
 
//------------------------------ metal7 user inductor device
INDUCTOR_M7 = ((M7 AND M7_markerLA) OR ( M7 AND  M7_markerLS)) OR ( M7 AND M7_markerL)         
INDUCTOR_ALUCAP_ML = ALUCAP AND AP_markerL        
INDUCTOR_ALUCAP_MLS = ALUCAP AND AP_markerLA        
INDUCTOR_ALUCAP_MLA = ALUCAP AND AP_markerLS    
INDUCTOR_ALUCAP = (INDUCTOR_ALUCAP_ML OR INDUCTOR_ALUCAP_MLS) OR INDUCTOR_ALUCAP_MLA    //select all the alucap with inductors marker

RI.DUM.D.1a {@ Rule I.DUM.D.1a: Minimum Metal Tiles Distance to Metal : 13
     
    EXT ME1_dy INDUCTOR_M1 < 13 ABUT <90 INSIDE ALSO SINGULAR
     
    EXT ME1_dy INDUCTOR_M2 < 13 ABUT <90 INSIDE ALSO SINGULAR
     
    EXT ME1_dy INDUCTOR_M3 < 13 ABUT <90 INSIDE ALSO SINGULAR
     
    EXT ME1_dy INDUCTOR_M4 < 13 ABUT <90 INSIDE ALSO SINGULAR
     
    EXT ME1_dy INDUCTOR_M5 < 13 ABUT <90 INSIDE ALSO SINGULAR
     
    EXT ME1_dy INDUCTOR_M6 < 13 ABUT <90 INSIDE ALSO SINGULAR
     
    EXT ME1_dy INDUCTOR_M7 < 13 ABUT <90 INSIDE ALSO SINGULAR
        EXT ME1_dy INDUCTOR_ALUCAP < 13 ABUT <90 INSIDE ALSO SINGULAR
}

RI.AP.W.1 {@ Rule I.AP.W.1: Maximum AP width within INDDMY : 60
    X = ALUCAP AND INDDMY
    X  WITH WIDTH > 60 
}


#IFDEF SHRINK_COMPATIBILITY

RCO.D.5_rec {@ Rule CO.D.5_rec: Recommended Distance of CO on OD to Gate on OD2 for a 10% shrink compatibility: 0.13
    EXT COOD_GP HV_GATE < 0.13 ABUT <90 INSIDE ALSO SINGULAR
    }
#ENDIF


