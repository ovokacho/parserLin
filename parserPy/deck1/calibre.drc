CNWPO_COREGO2:0_C1 = ( CNWPO_CORE:3_C1 INSIDE OD2_25_dg ) OR ( CNWPO_CORE:3_C1 INSIDE OD2_33_dg )
CNWPO_COREGO2:1_C1 = COPY CNWPO_COREGO2:0_C1            // model or user capa
CNWPO_2V5_C1 = ( CNWPO_COREGO2:1_C1 INSIDE OD2_25_dg ) OUTSIDE OD2_33_dg
CNWPO_3V3_C1 = ( CNWPO_COREGO2:1_C1 INSIDE OD2_33_dg ) OUTSIDE OD2_25_dg
CNWPO_C1 = POLYGi INTERACT (CNWPO_2V5_C1 OR CNWPO_3V3_C1)

CPWPO_ACT_C1 = ( ODi NOT NWELi ) INSIDE PPi    // active area
CPWPO_BOT_C1 = CPWPO_ACT_C1 ENCLOSE ACTPOL                    // bottom plate
CPWPO_CORE:0_C1 = CPWPO_BOT_C1 AND ACTPOL                    // capa area
CPWPO_CORE:1_C1 = CPWPO_CORE:0_C1 OUTSIDE NPi
CPWPO_CORE:2_C1 = CPWPO_CORE:1_C1 OUTSIDE ADHVTN
CPWPO_CORE:3_C1 = CPWPO_CORE:2_C1 OUTSIDE ADHVTP
CPWPO_COREGO2:0_C1 = ( CPWPO_CORE:3_C1 INSIDE OD2_25_dg ) OR ( CPWPO_CORE:3_C1 INSIDE OD2_33_dg )
CPWPO_2V5_C1 = ( CPWPO_COREGO2:0_C1 INSIDE OD2_25_dg ) OUTSIDE OD2_33_dg
CPWPO_3V3_C1 = ( CPWPO_COREGO2:0_C1 INSIDE OD2_33_dg ) OUTSIDE OD2_25_dg
CPWPO_C1 = POLYGi INTERACT (CPWPO_2V5_C1 OR CPWPO_3V3_C1)

CAPAPOLYWELL_C1 = CNWPO_C1 OR CPWPO_C1
COi_CAPAWELL = COi OR EMPTY
CONNECT POLYGi CAPAPOLYWELL_C1 BY COi_CAPAWELL

VIA_STACKED    = (VIA2i AND VIA3i) AND VIA4i
POSSIBLE_evenFRINGE:0    = M2i AND M4i
POSSIBLE_oddFRINGE:0    = M3i AND M5i
POSSIBLE_evenFRINGE:1    = POSSIBLE_evenFRINGE:0 INTERACT POSSIBLE_oddFRINGE:0 >=5
POSSIBLE_oddFRINGE:1    = POSSIBLE_oddFRINGE:0 INTERACT POSSIBLE_evenFRINGE:1 >=5
POSSIBLE_evenFRINGE:2  = POSSIBLE_evenFRINGE:1 INTERACT VIA_STACKED
POSSIBLE_oddFRINGE:2    = POSSIBLE_oddFRINGE:1 INTERACT VIA_STACKED

FRINGE_VIAS    = (POSSIBLE_evenFRINGE:2 AND POSSIBLE_oddFRINGE:2) INTERACT VIA_STACKED
FRINGE_NOVIAS    = (POSSIBLE_evenFRINGE:2 AND POSSIBLE_oddFRINGE:2) NOT FRINGE_VIAS
ZONE_1NOVIA    = ((POSSIBLE_evenFRINGE:2 OR POSSIBLE_oddFRINGE:2) NOT FRINGE_VIAS) TOUCH FRINGE_VIAS >1
ZONE_1VIA    = ((POSSIBLE_evenFRINGE:2 OR POSSIBLE_oddFRINGE:2) NOT FRINGE_NOVIAS) TOUCH FRINGE_NOVIAS >1
BAD_ZONE_MOMRF    = (ZONE_1NOVIA ENCLOSE FRINGE_NOVIAS !=1) OR (ZONE_1VIA ENCLOSE FRINGE_VIAS !=1)
ZONE_MOMRF    = (SIZE ((POSSIBLE_evenFRINGE:2 OR POSSIBLE_oddFRINGE:2 ) NOT INTERACT BAD_ZONE_MOMRF) BY 0.07 OVERUNDER) AND MKR_momdmy

inputs_MOMRF    = (((M4i OR M5i ) NOT ((M4i AND M5i ) AND M6i )) INTERACT ZONE_MOMRF) NOT ZONE_MOMRF
bus_MOMRF    = ((M4i AND M5i ) AND M6i ) TOUCH inputs_MOMRF
evenFRINGE    = (POSSIBLE_evenFRINGE:2 AND ZONE_MOMRF) NOT bus_MOMRF
oddFRINGE    = (POSSIBLE_oddFRINGE:2 AND ZONE_MOMRF) NOT bus_MOMRF

ODshield_MOMRF    = ((ODi AND NWELi) AND NPi) ENCLOSE oddFRINGE
POshield_MOMRF    = ((POLYGi AND NWELi) AND NPi) ENCLOSE oddFRINGE
MOMRF_SHIELD    = (ZONE_MOMRF INTERACT ODshield_MOMRF) INTERACT POshield_MOMRF
ODPOshield_MOMRF    = SIZE (ODshield_MOMRF OR POshield_MOMRF) BY  0.05 OVERUNDER
SHIELD_PIN_MOMRF    = (ODPOshield_MOMRF AND M1i) INTERACT (ODPOshield_MOMRF AND COi)
