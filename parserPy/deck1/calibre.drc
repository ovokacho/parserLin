// ----------------------------------------------------------------------------
//        minimum pad opening window  for  DEVICE  CLASS  B1
// ----------------------------------------------------------------------------

R1_B1 { @ Rule 1 (Device Class B1) : minimum pad opening window >= 110.000 x 110.000
      PAD_NOTCH_FILLED = EXTENT_PADB1 OR (EXT PADB1 <= 3.000 NOTCH REGION)
    BPADR    = NOT RECTANGLE PAD_NOTCH_FILLED >= 110.000 BY >=110.000
      BWPADP   = INT PAD_NOTCH_FILLED < 110.000 REGION OPPOSITE ABUT>0<90
      GWPADP   = PAD_NOTCH_FILLED OUTSIDE BWPADP
    BLPADP:1 = INT [GWPADP] < 110.000 OPPOSITE ABUT>0<90
      BLPADP   = GWPADP WITH EDGE BLPADP:1 >2
      BPADR OR ( BWPADP OR BLPADP )
    }

R1imp_B1 {@  Rule R1imp (Device Class B1) : 45 degrees notches are forbidden on pad opening
    X = ANGLE (EXTENT_PADB1 NOT PADB1) ==45
    (EXTENT_PADB1 NOT PADB1) WITH EDGE X >1
    }

R3_B1 { @ Rule 3 (Device Class B1) : maximum size of chamfer in the corner of pad is 10.000um in both direction
    X= EXTENT_PADB1 NOT PADB1
    Y = COINCIDENT EDGE X EXTENT_PADB1
    LENGTH Y > 10.000
    }
RA_B1 { @ Rule A (Device Class B1) : upper metal min enclosure of nitride 5.000
      ENC PADB1 THICKMETAL_B1 < 4.995 OUTSIDE ALSO
    } 

RC_B1 { @ Rule C (Device Class B1) : min Al to Al distance : 30.000
      X = SIZE EXTENT_PADB1 BY 25.000
    Y = (THICKMETAL_B1 INTERACT PADB1) AND X
    Z = Y OR (HOLES Y)
    EXT Z < 30.000 SPACE
    } 

